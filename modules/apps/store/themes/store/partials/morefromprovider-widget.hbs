<div class="row">
    <div class="span3">
        <h4><i class="icon-time"></i>{{t "More "}} {{type}}{{t "s from '"}} {{overview_provider}}'</h4>
        {{#each assets}}
        <div class="span3">
            <a href="{{url "/assets"}}/{{../type}}/{{id}}"><img src="{{#if images_thumbnail}}{{images_thumbnail}}{{/if}}"></a>
        </div>
        <div class="span9">
            <strong><a href="{{url "/assets"}}/{{../type}}/{{id}}">{{attributes.overview_name}}</a></strong>
                            <span class="star-rating">
                            	<div class="asset-rating">
                                </div>
                            </span>
        </div>
        {{/each}}
    </div>
</div>